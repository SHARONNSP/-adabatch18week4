function largestOfFour(arr) {
  // You can do this!
  var results = [];
  for(var i=0;i<4;i++){
    var largestNum = arr[i][0];
    for(var j=0;j<4;j++){
        if(arr[i][j] > largestNum){
          largestNum = arr[i][j];
        }
    }
    results.push(largestNum);
  }
  
  return results;
}

largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
